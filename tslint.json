{
  "rules": {
    "bitwise": false,
    "classname": true,
    "curly": false,
    "debug": true,
    "dupkey": true,
    "duplicate-variable": true,
    "eofline": true,
    "eqeqeq": [true, "allow-null-check"],
    "evil": true,
    "forin": true,
    "indent": [true, 4],
    "labelpos": true,
    "label-undefined": true,
    "maxlen": [false, 140],
    "noarg": true,
    "noconsole": [true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
    ],
    "noconstruct": true,
    "nounreachable": true,
    "noempty": false,
    "oneline": [false,
        "check-open-brace",
        "check-catch",
        "check-else",
        "check-whitespace"
    ],
    "quotemark": [true, "double"],
    "radix": true,
    "semicolon": true,
    "sub": true,
    "trailing": true,
    "varname": false,
    "whitespace": [true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
    ]
  }
}